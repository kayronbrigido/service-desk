{
  "extends": "next/core-web-vitals",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "indent": ["warn", 2, "tab"],
    "sort-imports": "error",
    "no-var": "error",
    "no-unused-vars": "error"
  }
}
